{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "clocking": {
      "type": "integer"
    },
    "start": {
      "type": "integer"
    },
    "active": {
      "type": "boolean"
    },
    "last_execution": {
      "type": "integer"
    }
  },
  "required": ["name", "clocking", "start"],
  "additionalProperties": false
}