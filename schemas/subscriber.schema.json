{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "email": {
      "type": "string"
    },
    "registered": {
      "type": "integer"
    },
    "active": {
      "type": "boolean"
    },
    "events": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  },
  "required": ["email", "events"],
  "additionalProperties": false
}